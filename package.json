{
  "scripts": {
    "setup": "npm install && npm run setup:flow-typed && npm run build",
    "setup:flow-typed": "flow-typed install express@4.x.x jest@20.x.x",
    "start": "node ./build/bin/server.js",
    "build": "babel src -d build -D -s --module-roots src",
    "watch": "npm run build -- -w",
    "flow": "flow check src",
    "test": "jest -c jest.json",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm test -- --watchAll",
    "lint": "eslint src test",
    "precommit": "lint-staged && npm run flow"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "config": "^1.28.1",
    "express": "^4.15.5",
    "express-validator": "^3.2.1",
    "i18n": "^0.8.3",
    "inversify": "^4.9.0",
    "log4js": "^2.4.1",
    "mysql": "^2.15.0",
    "source-map-support": "^0.4.18",
    "typeorm": "0.1.0-alpha.32"
  },
  "devDependencies": {
    "autobind-decorator": "^2.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.13.1",
    "eslint-plugin-flowtype": "^2.40.1",
    "flow-bin": "^0.50.0",
    "flow-typed": "^2.2.3",
    "husky": "^0.14.3",
    "jasmine-node": "^1.14.5",
    "jest": "^20.0.4",
    "lint-staged": "^6.0.0",
    "pre-commit": "^1.2.2",
    "supertest": "^3.0.0"
  }
}
